# ===========================================
# Debate Training Environment Configuration
# ===========================================
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# --- API Keys ---

# HuggingFace token (for gated models like Llama)
HF_TOKEN=

# Tinker API key (if using Tinker service instead of local HF)
TINKER_API_KEY=

# --- Model Configuration ---

# Model to use (default: Qwen/Qwen2.5-3B-Instruct)
MODEL_NAME=Qwen/Qwen2.5-3B-Instruct

# Dataset (default: open-r1/DAPO-Math-17k-Processed)
DATASET_NAME=open-r1/DAPO-Math-17k-Processed

# --- Training Defaults ---

# Use cheap mode by default (1=yes, 0=no)
CHEAP_MODE=1

# Confirm every N steps (0=only at start)
CONFIRM_EVERY=5

# --- Output ---

# Where to save outputs
OUTPUT_DIR=outputs/tinker_debate
